{
  "name": "transducers",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "tdd": "npm test -- --watch",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "changelog": "conventional-changelog -p eslint -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "serve:coverage": "http-server coverage/ -o",
    "slides:build": "node slides.js",
    "slides:dev": "npm run slides:build & npm run slides:start & watch 'npm run slides:build' slides",
    "slides:start": "reveal-md -w --theme moon slides.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madoos/transducers-workshop.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/madoos/transducers-workshop/issues"
  },
  "homepage": "https://github.com/madoos/transducers-workshop#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "conventional-changelog-cli": "^2.0.11",
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "http-server": "^0.11.1",
    "husky": "^1.3.1",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.3",
    "prettier-eslint-cli": "^4.7.1",
    "ramda": "^0.26.1",
    "reveal-md": "^2.4.1",
    "watch": "^1.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --single-quote --bracket-spacing true --print-width 80 --semi --write",
      "eslint",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
